{
  "name": "csk-v-next",
  "version": "6.0.0",
  "private": true,
  "engines": {
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm",
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "dev:watch": "WATCH=true run-s pull:configuration next:dev",
    "dev": "run-s pull:locales next:dev",
    "next:dev": "next dev",
    "build": "run-s pull:configuration next:build",
    "next:build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "theme-pack:extract": "theme-pack extract",
    "init": "run-s uniform:push uniform:publish",
    "uniform:push": "run-s push:configuration push:data",
    "pull:configuration": "theme-pack pull",
    "push:configuration": "theme-pack push",
    "pull:locales": "theme-pack pull -l",
    "pull:dev": "uniform sync pull --config ./uniform.config.dev.ts",
    "push:dev": "uniform sync push --config ./uniform.config.dev.ts",
    "pull:data": "uniform sync pull",
    "push:data": "uniform sync push",
    "uniform:publish": "uniform context manifest publish",
    "scaffold:add": "csk-cli scaffold",
    "modules:init": "csk-cli init"
  },
  "dependencies": {
    "@uniformdev/canvas-next-rsc": "20.0.0",
    "@uniformdev/theme-pack": "2.0.4",
    "next": "15.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@uniformdev/cli": "20.0.0",
    "@uniformdev/csk-cli": "2.0.4",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}