{
  "name": "component-starter-kit",
  "version": "5.2.1",
  "private": true,
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.17.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "dev": "run-s pull:manifest dev:next",
    "dev:next": "next dev",
    "build:next": "next build",
    "build": "run-s pull:manifest build:next",
    "build:test": "export NEXT_PUBLIC_E2E_TEST=true && run-s pull:manifest build:next",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,scss}\"",
    "pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:pull": "uniform sync pull",
    "uniform:push": "uniform sync push",
    "uniform:publish": "uniform context manifest publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postinstall": "cd cli-run && npm install && npm run build && cd ../cli-scaffold && npm install && npm run build",
    "cli": "npm i && cd cli-run && node dist/index.mjs",
    "scaffold": "npm i && node cli-scaffold/dist/index.mjs"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@contentful/rich-text-types": "^16.2.0",
    "@uniformdev/canvas": "19.134.0",
    "@uniformdev/canvas-next": "19.134.0",
    "@uniformdev/canvas-react": "19.134.0",
    "@uniformdev/context": "19.134.0",
    "@uniformdev/context-next": "19.134.0",
    "@uniformdev/context-react": "19.134.0",
    "@uniformdev/project-map": "19.134.0",
    "@uniformdev/richtext": "19.134.0",
    "@vercel/og": "^0.5.7",
    "classnames": "^2.3.2",
    "cookies-next": "4.1.1",
    "daisyui": "^3.9.3",
    "framer-motion": "10.16.2",
    "next": "14.2.1",
    "next-intl": "^3.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-multi-carousel": "^2.8.3",
    "react-player": "^2.12.0",
    "react-responsive-masonry": "2.1.7",
    "sass": "1.64.1",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/addon-styling": "^1.0.3",
    "@storybook/blocks": "7.5.3",
    "@storybook/nextjs": "7.5.3",
    "@storybook/preview-api": "7.5.3",
    "@storybook/react": "7.5.3",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/color": "^3.0.3",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/react-responsive-masonry": "2.1.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/types": "^6.4.0",
    "@uniformdev/cli": "19.134.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.47.0",
    "eslint-config-custom": "*",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^3.0.2",
    "shared": "*",
    "storybook": "7.5.3",
    "storybook-addon-themes": "^6.1.0",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "5.3.3"
  }
}
